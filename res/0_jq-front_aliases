function docker-jq-front() {
  docker run --rm -i \\
    -v /:/var/lib/jf \\
    -e JF_PATH_BASE="/var/lib/jf" \\
    -e JF_PATH="$(echo '${JF_PATH}')" \\
    -e JF_DEBUG=$(echo '${JF_DEBUG:-disabled}') \\
    -e JF_CWD="$(echo '$(pwd)')" \\
    ${DOCKER_REPO_NAME}:"$(echo '${JF_DOCKER_TAG'):-${LATEST_RELEASED_VERSION}$(echo '}')" "$(echo '${@}')"
}
